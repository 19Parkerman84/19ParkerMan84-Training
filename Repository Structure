# Clone your repository
git clone https://github.com/19Parkerman84/19ParkerMan84-Training.git
cd 19ParkerMan84-Training

# Create directories
mkdir css js components pages assets assets/images

# Create necessary files
touch index.html
touch css/styles.css css/components.css css/utilities.css
touch js/main.js js/form-handler.js js/calendar.js js/news-fetcher.js
